<!DOCTYPE html>
<html>
<head>
  <title>Quiz Index</title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
  <h1>Interview questions</h1>
    <ul>
      <% questions.forEach(function(question) { %>
          <li><a href='/api/quiz/<%= question._id %>'><%= question.title %></a> - <%= question.categories %></li>
      <% }); %>
    </ul>

</body>
</html>
